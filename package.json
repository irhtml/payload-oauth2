{
  "name": "payload-oauth2",
  "version": "1.0.3",
  "type": "module",
  "homepage:": "https://payloadcms.com",
  "repository": "https://github.com/WilsonLe/payload-oauth2",
  "description": "OAuth2 plugin for Payload CMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "oauth2",
    "payload-plugin"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "tsc",
    "test": "cd test && jest --config=./jest.config.js",
    "lint": "eslint src",
    "format": "prettier --write src",
    "payload": "cd dev && cross-env NODE_OPTIONS=--no-deprecation payload",
    "dev:lint": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next lint",
    "generate:types": "payload generate:types",
    "generate:importmap": "payload generate:importmap",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "clean": "rimraf dist && rimraf dev/.next",
    "prepublishOnly": "pnpm clean && pnpm build",
    "prepare": "tsc"
  },
  "author": "wilsonle2907@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "payload": "3.0.0-beta.116"
  },
  "devDependencies": {
    "@payloadcms/db-mongodb": "3.0.0-beta.116",
    "@payloadcms/eslint-config": "^1.1.1",
    "@payloadcms/next": "3.0.0-beta.116",
    "@payloadcms/richtext-lexical": "3.0.0-beta.116",
    "@payloadcms/ui": "3.0.0-beta.116",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint": "9.5.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "jsonwebtoken": "9.0.2",
    "next": "15.0.0-canary.160",
    "payload": "3.0.0-beta.110",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "3.2.4",
    "react": "19.0.0-rc-5dcb0097-20240918",
    "react-dom": "19.0.0-rc-5dcb0097-20240918",
    "rimraf": "5.0.7",
    "sharp": "0.33.4",
    "typescript": "5.4.5"
  }
}
